{
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "tasks": [
        {
            "taskName": "lint",
            "isBuildCommand": true,
            "problemMatcher": "$eslint-compact",
            "args": [ "run" ]
        },
        {
            "taskName": "test",
            "isTestCommand": true
        },
        {
            "taskName": "tsd-reinstall",
            "args": [ "run" ]
        },
        {
            "taskName": "browserify",
            "args": [ "run" ]
        }
    ]
}