{
  "name": "sayveet",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "jasmine",
    "lint": "jshint .",
    "tsd-reinstall": "tsd reinstall --save",
    "postinstall": "./node_modules/bower/bin/bower install",
    "browserify": "npm run browserify-transactions & npm run browserify-reports",
    "browserify-transactions": "browserify public/javascripts/transactions/transactions.js > public/js/transactions.js",
    "browserify-reports": "browserify public/javascripts/transactions/reports.js > public/js/reports.js"
  },
  "dependencies": {
    "body-parser": "^1.12.4",
    "bower": "^1.4.1",
    "commander": "^2.8.1",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "express": "^4.13.0",
    "jade": "^1.10.0",
    "mongoose": "^4.0.5",
    "morgan": "^1.5.3",
    "serve-favicon": "^2.2.1"
  },
  "devDependencies": {
    "browserify": "^10.2.4",
    "browserify-shim": "^3.8.8",
    "jasmine": "^2.3.1",
    "jshint": "^2.8.0",
    "tsd": "^0.6.1"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$",
    "knockout": "global:ko",
    "underscore": "global:_",
    "moment": "global:moment",
    "bootstrap-datepicker": "global:bootstrapDatepicker",
    "c3": "global:c3"
  }
}
